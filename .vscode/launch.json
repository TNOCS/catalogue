{
    "version": "0.2.0",
    "configurations": [{
        "name": "Launch server",
        "type": "node",
        "request": "launch",
        "program": "${workspaceRoot}/json-server-auth/src/server.ts",
        "stopOnEntry": false,
        "args": [],
        "cwd": "${workspaceRoot}/json-server-auth",
        "preLaunchTask": null,
        "runtimeExecutable": null,
        "runtimeArgs": [
            "--nolazy"
        ],
        "env": {
            "NODE_ENV": "development"
        },
        "externalConsole": false,
        "sourceMaps": true,
        "outDir": "${workspaceRoot}/json-server-auth/src"
    }, {
        "name": "Attach",
        "type": "node",
        "request": "attach",
        "port": 5858,
        "address": "localhost",
        "restart": false,
        "sourceMaps": false,
        "outDir": null,
        "localRoot": "${workspaceRoot}",
        "remoteRoot": null
    }]
}